{
  "manifest_version": 3,
  "name": "Gemini Nano Demo",
  "version": "1.0",
  "description": "A simple Gemini Nano Demo built with React",
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "permissions": ["sidePanel", "storage", "tabs", "tts", "scripting", "activeTab"],
  "host_permissions": ["https://www.scribbr.com/", "<all_urls>"]

}